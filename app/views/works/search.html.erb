
        <main>
            <div id="main-inner">
                <div class="search-area search-result-area">
                    <h2 id="search-title">「<%=@title%>」の検索結果</h2>
                    <div id="keyword-search-area">
                        <%= form_with url: works_search_path, method: :get, local: true ,id:"search-form" do |form| %>
                            <%= text_field_tag :search,"",{id:"sbox1"} %>
                            <%= submit_tag '検索', :name => nil ,id:"sbtn1"%>
                        <% end %>

                    </div>
                </div>
                <div id="anime-search-result-area">
                    <% @works.each do |work| %>
                        <article class="my-anime-article">
                            <% my_work=current_user.ranked_by?(work.id) %>
                            <% if my_work %>
                                <%rank=my_work["rank"]%>
                                <% if rank==1 %>
                                    <div class="my-anime-rank-wrapper">
                                        <div class="my-anime-rank-area">
                                            <i class="fas fa-crown bronze crown"></i>
                                            <p class="my-anime-rank">Rank</p>
                                            <p class="my-anime-rank-result bronze">Bronze</p>
                                        </div>
                                        <div class="my_anime_rank_update-area">
                                            <i class="fas fa-sync-alt"></i>
                                            <%= link_to("/my_work/gold/#{work.id}", {method: "post"})do%><i class="fas fa-crown gold crown"></i><% end %>
                                            <%= link_to("/my_work/silver/#{work.id}", {method: "post"})do%><i class="fas fa-crown silver crown"></i><% end %>
                                            <%= link_to("/my_work/delete/#{work.id}", {method: "delete"})do%><i class="fas fa-trash-alt trash"></i><% end %>
                                        </div>   
                                    </div>
                                <% elsif rank==2%>
                                    <div class="my-anime-rank-wrapper">
                                        <div class="my-anime-rank-area">
                                            <i class="fas fa-crown silver crown"></i>
                                            <p class="my-anime-rank ">Rank</p>
                                            <p class="my-anime-rank-result silver">Silver</p>
                                        </div>
                                        <div class="my_anime_rank_update-area">
                                            <i class="fas fa-sync-alt"></i>
                                            <%= link_to("/my_work/gold/#{work.id}", {method: "post"})do%><i class="fas fa-crown gold crown"></i><% end %>
                                            <%= link_to("/my_work/bronze/#{work.id}", {method: "post"})do%><i class="fas fa-crown bronze crown"></i><% end %>
                                            <%= link_to("/my_work/delete/#{work.id}", {method: "delete"})do%><i class="fas fa-trash-alt trash"></i><% end %>
                                        </div>   
                                    </div>
                                <% elsif rank==3%>
                                    <div class="my-anime-rank-wrapper">
                                        <div class="my-anime-rank-area">
                                            <i class="fas fa-crown gold crown"></i>
                                            <p class="my-anime-rank">Rank</p>
                                            <p class="my-anime-rank-result gold">Gold</p>
                                        </div>
                                        <div class="my_anime_rank_update-area">
                                            <i class="fas fa-sync-alt"></i>
                                            <%= link_to("/my_work/silver/#{work.id}", {method: "post"})do%><i class="fas fa-crown silver crown"></i><% end %>
                                            <%= link_to("/my_work/bronze/#{work.id}", {method: "post"})do%><i class="fas fa-crown bronze crown"></i><% end %>
                                            <%= link_to("/my_work/delete/#{work.id}", {method: "delete"})do%><i class="fas fa-trash-alt trash"></i><% end %>
                                        </div>   
                                    </div>
                                <%end%>
                            <% else%>
                                <div class="my-anime-rank-wrapper">
                                        <div class="my-anime-rank-area">
                                            <p class="my-anime-rank">Rank</p>
                                            <p class="my-anime-bronz">None</p>
                                        </div>
                                        <div class="my_anime_rank_update-area">
                                            <i class="fas fa-plus "></i>
                                            <%= link_to("/my_work/gold/#{work.id}", {method: "post"})do%><i class="fas fa-crown gold crown"></i><% end %>
                                            <%= link_to("/my_work/silver/#{work.id}", {method: "post"})do%><i class="fas fa-crown silver crown"></i><% end %>
                                            <%= link_to("/my_work/bronze/#{work.id}", {method: "post"})do%><i class="fas fa-crown bronze crown"></i><% end %>
                                        </div>   
                                    </div>
                            <% end %>
                            <div class="my-anime-text-area">
                                <p class="my-anime-title"><%= work.title %></p>
                            </div>
                        </article>
                    <% end%>
                </div>
            </div>
        </main>